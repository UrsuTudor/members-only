<%= link_to 'New Post', new_post_path %>

<ul>
  <% @posts.each   do |post| %>
  <li>
  <% if user_signed_in? %>
    <%= User.find(post.user_id).email%>
    <% end %>
  <%= link_to post.title, post%>
  </li>
  <% end %>
</ul>

<%= button_to "Log Out", destroy_user_session_path, method: :delete  %>
<%= button_to "Log In", user_session_path %>

